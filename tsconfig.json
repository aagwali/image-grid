{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "module": "es2015",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true
  },
  "exclude": ["node_modules", "dist"]
}
